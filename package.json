  "description": "Formats microseconds into a human consumable form.",
  "license": "MIT",
  "repository": "cullylarson/formatmicro",
  "author": {
    "name": "Cully Larson",
    "email": "cully.larson@gmail.com"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
